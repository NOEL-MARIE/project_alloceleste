{
  "extends": [
    "development"
  ],
  "hints": {
    "typescript-config/strict": "off",
    "typescript-config/consistent-casing": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-moz-appearance"
        ]
      }
    ],
    "typescript-config/is-valid": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 130",
    "not chrome <= 130",
    "not edge <= 130",
    "not ios_saf <= 18.1",
    "not opera <= 114",
    "not safari <= 18.1",
    "not samsung <= 26"
  ]
}